        "arch": "x64",
        "service_pack": "",
        "installer": [
        ],
        "build_steps": {
          "configure": [],
          "pre_build": [],
          "build": [
            "cd ../;make build"
          ],
          "post_build": [
            "cp -R ../build/code/* /usr/share/nginx/html/",
            "chmod -R 755 /usr/share/nginx/html/",
            "mkdir /usr/share/nginx/html/logs",
            "chmod -R 777 /usr/share/nginx/html/logs"
          ],
          "status": []
        }
      }
    },
    "runtime_requirements": {
      "platform": {
        "os": "ubuntu",
        "osVersion": "14.04",
        "arch": "x64",
        "servicepack": "",
        "hosting": "dedicated",
        "memory": {
          "max_required": "4gb",
          "min_required": "2gb"
        },
        "storage": {
          "min_required": "10gb"
        },
        "installer": [
          
	  "apt-get update -y",
	  "apt-get install python-pip python-dev nginx",
	  "python /usr/share/nginx/html/deployment/setup.py install",
	  "cp /usr/share/nginx/html/deployment/lds.conf /etc/init/",
	  "start lds",
	  "cp /usr/share/nginx/html/deployment/lds /etc/nginx/sites-available/",
	  "ln -s /etc/nginx/sites-available/lds /etc/nginx/sites-enabled",
	  "rm -v /etc/nginx/sites-enabled/default",
	  "nginx -t",
	  "service nginx restart"
       
        ],
